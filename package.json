{
  "name": "realworld-slices",
  "private": true,
  "version": "1.0.0",
  "description": "A Realworld.io backend following SOLID architecture in slices, not layers.",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build && env-cmd node dist/index.js",
    "build": "tsc",
    "migration:clean": "rimraf ./dist/migrations",
    "migration:revert": "npm run build && typeorm migration:revert",
    "migration:generate": "npm run migration:run && typeorm migration:generate -n",
    "migration:run": "npm run migration:clean && npm run build && typeorm migration:run"
  },
  "keywords": [],
  "repository": "https://github.com/jonathonflorek/realworld-slices",
  "author": "Jonathon Florek",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.17.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.22",
    "validate-typescript": "^4.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/express-jwt": "0.0.42",
    "@types/jsonwebtoken": "^8.3.6",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.0.1",
    "rimraf": "^3.0.0",
    "sqlite3": "^4.1.1",
    "typescript": "^3.7.4"
  }
}
